@model eShop.Models.MasterSalesPerson

@{
    Layout = null;
}


<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <h4 class="modal-title">Details - Master Sales</h4>
        </div>
        <div class="modal-body">
            <div id="divEmp">
                <div class="nav-tabs-custom">
                    <ul class="nav nav-tabs">
                        <li class="active"><a href="#tab_1" data-toggle="tab">General</a></li>
                        <li><a href="#tab_2" data-toggle="tab">Alamat</a></li>
                        <li><a href="#tab_3" data-toggle="tab">Pajak</a></li>
                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane active form-horizontal" id="tab_1">
                            <div class="form-group">
                                <div class="col-md-10">
                                    @Html.LabelFor(model => model.Code)
                                    @Html.TextBoxFor(model => model.Code, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Code, "", new { @class = "text-danger" })
                                </div>
                                <div class="col-md-2 text-center">
                                    @Html.LabelFor(model => model.Active)
                                    <div class="checkbox text-center">
                                        @Html.CheckBoxFor(model => model.Active)
                                        @Html.CheckBoxFor(model => model.Active, new { disabled = "disabled" })
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-8">
                                    @Html.LabelFor(model => model.Name)
                                    @Html.TextBoxFor(model => model.Name, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Name, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-4" id="gender">
                                    @Html.LabelFor(model => model.Gender)
                                    @Html.TextBoxFor(model => model.Gender, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Gender, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-4">
                                    @Html.LabelFor(model => model.Phone1)
                                    @Html.TextBoxFor(model => model.Phone1, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Phone1, "", new { @class = "text-danger" })
                                </div>
                                <div class="col-md-4">
                                    @Html.LabelFor(model => model.Phone2)
                                    @Html.TextBoxFor(model => model.Phone2, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Phone2, "", new { @class = "text-danger" })
                                </div>
                                <div class="col-md-4">
                                    @Html.LabelFor(model => model.Mobile)
                                    @Html.TextBoxFor(model => model.Mobile, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Mobile, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-4">
                                    @Html.LabelFor(model => model.Email)
                                    @Html.TextBoxFor(model => model.Email, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Email, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="tab-pane form-horizontal" id="tab_2">
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.LabelFor(model => model.Address)
                                    @Html.TextBoxFor(model => model.Address, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Address, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-4">
                                    @Html.LabelFor(model => model.Birthday)
                                    @Html.TextBoxFor(model => model.Birthday, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Birthday, "", new { @class = "text-danger" })
                                </div>
                                <div class="col-md-4">
                                    @Html.LabelFor(model => model.City)
                                    @Html.TextBoxFor(model => model.City, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.City, "", new { @class = "text-danger" })
                                </div>
                                <div class="col-md-4">
                                    @Html.LabelFor(model => model.Postal)
                                    @Html.TextBoxFor(model => model.Postal, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Postal, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.LabelFor(model => model.Notes)
                                    @Html.TextBoxFor(model => model.Notes, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Notes, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="tab-pane form-horizontal" id="tab_3">
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.LabelFor(model => model.IDCard)
                                    @Html.TextBoxFor(model => model.IDCard, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.IDCard, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.LabelFor(model => model.TaxID)
                                    @Html.TextBoxFor(model => model.TaxID, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.TaxID, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.LabelFor(model => model.TaxName)
                                    @Html.TextBoxFor(model => model.TaxName, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.TaxName, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.LabelFor(model => model.TaxAddress)
                                    @Html.TextBoxFor(model => model.TaxAddress, new { disabled = "disabled", @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.TaxAddress, "", new { @class = "text-danger" })
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <div class="text-right">
                    <button type="button" class="btn btn-danger btn-block btn-lg" data-dismiss="modal">
                        Close
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

