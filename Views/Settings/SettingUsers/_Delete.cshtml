@model eShop.Models.ApplicationUser

    @{
        Layout = null;
    }

    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title">Apakah anda yakin untuk menghapus data ini?</h4>
            </div>
            <div class="modal-body">
                <div id="divEmp">
                    @using (Ajax.BeginForm("Delete", "SettingUsers", new AjaxOptions { HttpMethod = "POST", UpdateTargetId = "divEmp", OnSuccess = "DeleteSuccess" }))
                    {
                        @Html.AntiForgeryToken()
                        @Html.HiddenFor(model => model.Id)
                    <div class="form-horizontal">
                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                        <div class="form-group">
                            <div class="col-md-12">
                                <table align="left">
                                    <tr>
                                        <td style="font-weight:bold;" width="130px" valign="top">
                                            Username
                                        </td>
                                        <td style="font-weight:bold;" valign="top">
                                            &nbsp;:&nbsp;
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.UserName)
                                        </td>
                                    </tr>
                                    <tr>
                                        <td style="font-weight:bold;" width="130px" valign="top">
                                            &nbsp;
                                        </td>
                                        <td style="font-weight:bold;" valign="top">
                                            &nbsp;
                                        </td>
                                        <td>
                                            &nbsp;
                                        </td>
                                    </tr>
                                    <tr>
                                        <td style="font-weight:bold;" valign="top">
                                            @Html.DisplayNameFor(model => model.Email)
                                        </td>
                                        <td style="font-weight:bold;" valign="top">
                                            &nbsp;:&nbsp;
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.Email)
                                        </td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-12 text-right">
                                <input type="submit" value="Delete" class="btn btn-danger" />
                            </div>
                        </div>
                    </div>
                    }
                </div>
            </div>
        </div>
    </div>